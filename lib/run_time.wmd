wm.table_pub:
  fields:
  - {col: obj, size: 40}
  - {col: kind, size: 4}
  - {col: pkey, size: 40}
  - {col: help, size: 60}
  display: [obj, kind, cols, pkey, title, help]
  where: {language: 'eng'}

