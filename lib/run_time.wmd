---
wm.table_text:
  focus: code
  fields:
  - {col: language, size: 4}
  - {col: title, size: 40}
  - {col: help, size: 40, spe: edw}

wm.column_text:
  focus: code
  fields:
  - {col: language, size: 4}
  - {col: title, size: 40}
  - {col: help, size: 40, spe: edw}

wm.value_text:
  focus: code
  fields:
  - {col: language, size: 4}
  - {col: title, size: 40}
  - {col: help, size: 40, spe: edw}

wm.message_text:
  focus: code
  fields:
  - {col: language, size: 4}
  - {col: title, size: 40}
  - {col: help, size: 40, spe: edw}

wm.column_pub:
  focus: code
  fields:
  - {col: language, size: 4}
  - {col: title, size: 40}
  - {col: help, size: 40, spe: edw}

wm.table_data:
  fields:
  - {col: td_sch, size: 10}
  - {col: td_tab, size: 30}
  - {col: obj, size: 40}
  - {col: kind, size: 4}
